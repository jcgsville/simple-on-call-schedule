{
    "name": "simple-on-call-schedule",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build:deployment": "npm-run-all -s build:*",
        "build:remix": "remix build",
        "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
        "build:dev-server": "cross-env NODE_ENV=development npm run build:server -- --watch",
        "dev": "npm-run-all -p build:dev-server dev:remix",
        "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint --ext .ts,.tsx .",
        "lint:fix": "eslint --ext .ts,.tsx --fix .",
        "start": "cross-env NODE_ENV=production node ./build/server.js",
        "type-check": "tsc"
    },
    "dependencies": {
        "@remix-run/css-bundle": "2.3.1",
        "@remix-run/express": "2.3.1",
        "@remix-run/node": "2.3.1",
        "@remix-run/react": "2.3.1",
        "cross-env": "7.0.3",
        "isbot": "3.7.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tiny-invariant": "1.3.1",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@remix-run/dev": "2.3.1",
        "@remix-run/eslint-config": "2.3.1",
        "@testing-library/jest-dom": "6.1.5",
        "@testing-library/react": "14.1.2",
        "@types/express": "4.17.21",
        "@types/react": "18.2.42",
        "@types/react-dom": "18.2.17",
        "@types/uuid": "9.0.7",
        "eslint": "8.55.0",
        "jest": "29.7.0",
        "npm-run-all": "4.1.5",
        "prettier": "3.1.0",
        "prettier-eslint": "16.1.2",
        "typescript": "5.1.6"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
